<?xml version="1.0" encoding="utf-8"?>
<configuration>
  <system.webServer>
    <handlers>
      <add name="httpPlatformHandler" path="*" verb="*" 
           modules="httpPlatformHandler" resourceType="Unspecified"/>
    </handlers>
    <httpPlatform 
      processPath="%JAVA_HOME%\bin\java.exe"
      arguments="-Djava.net.preferIPv4Stack=true -jar &quot;%HOME%\site\wwwroot\dropwizard-1.0.jar&quot; server &quot;%HOME%\site\wwwroot\config.yml&quot;"
      stdoutLogEnabled="true"
      stdoutLogFile="C:\home\LogFiles\stdLogs\stdout.log" />
  </system.webServer>
</configuration>
